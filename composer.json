{
  "name": "gonzaloner/laravel-trustspot",
  "description": "A very basic TrustSpot API client wrapper for Laravel",
  "keywords": ["trustspot", "api", "php", "laravel"],
  "license": "MIT",
  "authors": [
    {
      "name": "Gonzalo Faus Agull√≥",
      "email": "gfausagullo@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "type": "project",
  "require": {
    "gonzaloner/trustspot-api-php": "0.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.2.*"
  },
  "autoload": {
    "psr-4": {
      "Gonzaloner\\LaravelTrustSpot\\": "src/"
    }
  },
  "extra": {
    "laravel": {
